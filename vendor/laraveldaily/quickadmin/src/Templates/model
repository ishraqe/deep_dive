    $SOFT_DELETE_USE$

    $SOFT_DELETE_DATES$

    protected $table    = '$TABLENAME$';
    
    protected $fillable = [$FILLABLE$];
    $ENUMS$

    public static function boot()
    {
        parent::boot();

        $CLASS$::observe(new UserActionsObserver);
    }
    $RELATIONSHIPS$
    $PASSWORDHASH$
    $DATEPICKERS$
    $DATETIMEPICKERS$
